{
  "name": "rosspalmer",
  "version": "3.0.0",
  "description": "Personal website",
  "author": "Ross Palmer",
  "license": "ISC",
  "main": "index.html",
  "repository": {
    "type": "git",
    "url": "https://github.com/rjpr/portfolio"
  },
  "scripts": {
    "scss": "mkdir -p css/ && sass scss/:css/",
    "scss-watch": "sass --watch scss/:css/",
    "prefix": "postcss css/styles.css --use autoprefixer -d css/ --no-map",
    "build-css": "npm run scss && npm run prefix",
    "build-html": "inline-source --compress index.html index.out.html",
    "build": "npm run build-css && npm run build-html && bash buildfile.sh",
    "deploy": "npm run build && gh-pages -d build"
  },
  "dependencies": {
    "autoprefixer": "^10.4.22",
    "inline-source-cli": "^2.0.0",
    "sass": "^1.94.2",
    "postcss-cli": "^11.0.1"
  },
  "devDependencies": {
    "gh-pages": "^6.3.0"
  }
}